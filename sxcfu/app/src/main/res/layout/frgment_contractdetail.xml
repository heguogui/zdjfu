<?xml version="1.0" encoding="utf-8"?>
<com.hz.zdjfu.application.widget.view.DragCustInfoScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/product_detail_scrollView"
    android:layout_marginTop="@dimen/dimen_5_dip"
    android:orientation="vertical"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/dimen_10_dip"
            android:background="@color/white0"
            >
            <TextView
                android:id="@+id/contractdetail_view1"
                android:layout_width="@dimen/dimen_5_dip"
                android:layout_height="@dimen/dimen_15_dip"
                android:background="@color/green1"
                android:layout_marginLeft="@dimen/dimen_16_dip"
                android:layout_marginTop="@dimen/dimen_12_dip"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/colorBack3"
                android:layout_marginLeft="@dimen/dimen_34_dip"
                android:layout_marginTop="@dimen/dimen_12_dip"
                android:textSize="15sp"
                android:text="合同协议"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/dimen_20_dip"
                android:visibility="gone"
                />

            <com.hz.zdjfu.application.widget.view.MyGridView
                android:id="@+id/contractdetail_gv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/contractdetail_view1"
                android:layout_marginLeft="@dimen/dimen_17_dip"
                android:layout_marginRight="@dimen/dimen_17_dip"
                android:layout_marginTop="@dimen/dimen_24_dip"
                android:numColumns="2"
                >
            </com.hz.zdjfu.application.widget.view.MyGridView>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/dimen_10_dip"
            android:background="@color/white0"
            android:layout_marginTop="@dimen/dimen_5_dip"
            >
            <TextView
                android:id="@+id/contractdetail_view2"
                android:layout_width="@dimen/dimen_5_dip"
                android:layout_height="@dimen/dimen_15_dip"
                android:background="@color/green1"
                android:layout_marginLeft="@dimen/dimen_16_dip"
                android:layout_marginTop="@dimen/dimen_12_dip"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/colorBack3"
                android:layout_marginLeft="@dimen/dimen_34_dip"
                android:layout_marginTop="@dimen/dimen_12_dip"
                android:textSize="15sp"
                android:text="其他文件"
                />

            <com.hz.zdjfu.application.widget.view.MyGridView
                android:id="@+id/contractdetail_other_gv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/contractdetail_view2"
                android:layout_marginLeft="@dimen/dimen_17_dip"
                android:layout_marginRight="@dimen/dimen_17_dip"
                android:layout_marginTop="@dimen/dimen_24_dip"
                android:numColumns="2"
                >
            </com.hz.zdjfu.application.widget.view.MyGridView>
        </RelativeLayout>


    </LinearLayout>





</com.hz.zdjfu.application.widget.view.DragCustInfoScrollView>
